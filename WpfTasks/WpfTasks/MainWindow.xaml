<Window x:Class="WpfTasks.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfTasks"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Name="MyButton" Content="Button" FontSize="32" HorizontalAlignment="Left" Margin="32,10,0,0" VerticalAlignment="Top" Width="132" Height="59" Click="MyButton_Click2"/>
        <WebBrowser Name="MyWebBrowser" HorizontalAlignment="Left" Height="279" Margin="10,105,0,0" VerticalAlignment="Top" Width="772" RenderTransformOrigin="0.5,0.5">
            <WebBrowser.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleY="-0.224"/>
                    <RotateTransform/>
                    <TranslateTransform Y="-0.55"/>
                </TransformGroup>
            </WebBrowser.RenderTransform>
        </WebBrowser>

    </Grid>
</Window>
